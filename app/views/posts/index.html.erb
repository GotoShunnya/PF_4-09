<h1>index</h1>

<% @posts.each do |posts| %>
  <table>
    <thead>
      <tr>
        <th>投稿者</th>
        <th>Title</th>
        <th>Image</th>
        <th>投稿日時</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= posts.user.name %></td>
        <td><%= link_to posts.title, post_path(posts.id) %></td>
        <td><%= attachment_image_tag posts, :image, fallback: "no_image.jpg", format:"jpeg", size:"200x200" %></td>
        <td><%= posts.created_at %></td>
      </tr>
    </tbody>
  </table>
<% end %>
